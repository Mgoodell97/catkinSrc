<!-- mission file for GADEN player -->

<launch>

    ### NODE Simulated Gas Sensor ###
    #--------------------------------
    # SENSOR MODELS:
    # MOX: TGS2620=0, TGS2600=1, TGS2611=2
    # PID: MiniRaeLite=30

    # sixth row of 5 X = 2_56m
    <node pkg="tf" type="static_transform_publisher" name="pid25_broadcaster" args="2.56 0.46 0.3 0 0 0 map pid25_frame 50" />
    <node pkg="tf" type="static_transform_publisher" name="pid26_broadcaster" args="2.56 0.91 0.3 0 0 0 map pid26_frame 50" />
    <node pkg="tf" type="static_transform_publisher" name="pid27_broadcaster" args="2.56 1.37 0.3 0 0 0 map pid27_frame 50" />
    <node pkg="tf" type="static_transform_publisher" name="pid28_broadcaster" args="2.56 1.83 0.3 0 0 0 map pid28_frame 50" />
    <node pkg="tf" type="static_transform_publisher" name="pid29_broadcaster" args="2.56 2.29 0.3 0 0 0 map pid29_frame 50" />


    # sixth row of 5 X = 2_56m
    #                X    Y    Z
    <group ns="PID__2_56__0_46__0_3">
        <node pkg="simulated_gas_sensor" type="simulated_gas_sensor" name="fake_pid" output="screen">
	        <param name="/sensor_model" value="30" />
	        <param name="/sensor_frame" value="pid25_frame" />
	        <param name="/fixed_frame" value="map"/>
        </node>
    </group>

    <group ns="PID__2_56__0_91__0_3">
        <node pkg="simulated_gas_sensor" type="simulated_gas_sensor" name="fake_pid" output="screen">
	        <param name="/sensor_model" value="30" />
	        <param name="/sensor_frame" value="pid26_frame" />
	        <param name="/fixed_frame" value="map"/>
        </node>
    </group>

    <group ns="PID__2_56__1_37__0_3">
        <node pkg="simulated_gas_sensor" type="simulated_gas_sensor" name="fake_pid" output="screen">
	        <param name="/sensor_model" value="30" />
	        <param name="/sensor_frame" value="pid27_frame" />
	        <param name="/fixed_frame" value="map"/>
        </node>
    </group>

    <group ns="PID__2_56__1_83__0_3">
        <node pkg="simulated_gas_sensor" type="simulated_gas_sensor" name="fake_pid" output="screen">
          <param name="/sensor_model" value="30" />
          <param name="/sensor_frame" value="pid28_frame" />
          <param name="/fixed_frame" value="map"/>
        </node>
    </group>

    <group ns="PID__2_56__2_29__0_3">
        <node pkg="simulated_gas_sensor" type="simulated_gas_sensor" name="fake_pid" output="screen">
          <param name="/sensor_model" value="30" />
          <param name="/sensor_frame" value="pid29_frame" />
          <param name="/fixed_frame" value="map"/>
        </node>
    </group>

    ### RQT PLOT Sensors ###
    #-----------------------
    <node pkg="rqt_plot" type="rqt_plot" name="rqt_plot6"  args="/PID__2_56__0_46__0_3/Sensor_reading/raw /PID__2_56__0_91__0_3/Sensor_reading/raw /PID__2_56__1_37__0_3/Sensor_reading/raw /PID__2_56__1_83__0_3/Sensor_reading/raw /PID__2_56__2_29__0_3/Sensor_reading/raw"/>


</launch>
